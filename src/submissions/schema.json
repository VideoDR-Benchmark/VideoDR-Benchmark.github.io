{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "VideoDR Model Performance",
  "description": "Submit your model performance data to the leaderboard",
  "properties": {
    "Framework": {
      "type": "string",
      "title": "Framework",
      "description": "AI framework name (e.g., Workflow, Agentic)"
    },
    "LLM": {
      "type": "string",
      "title": "LLM",
      "description": "Large language model name (e.g., GPT-4o, Gemini-3-pro)"
    },
    "ACC": {
      "type": "number",
      "title": "ACC (%)",
      "description": "Accuracy (0-100)",
      "minimum": 0,
      "maximum": 100
    },
    "ThinkCount": {
      "type": "number",
      "title": "Think Calls",
      "description": "Average number of think calls",
      "minimum": 0
    },
    "SearchCount": {
      "type": "number",
      "title": "Search Calls",
      "description": "Average number of search calls",
      "minimum": 0
    },
    "TotalTime": {
      "type": "number",
      "title": "Total Time (s)",
      "description": "Average total execution time in seconds",
      "minimum": 0
    },
    "SiteUrl": {
      "type": "string",
      "title": "Website",
      "description": "Model website URL"
    }
  },
  "required": [
    "Framework",
    "LLM",
    "ACC",
    "ThinkCount",
    "SearchCount",
    "TotalTime",
    "SiteUrl"
  ],
  "examples": [
    {
      "Framework": "Workflow",
      "LLM": "Qwen3-Omni-30B-A3B",
      "ACC": 37.0,
      "ThinkCount": 1.82,
      "SearchCount": 0.95,
      "TotalTime": 222.86,
      "SiteUrl": "https://qwen.ai/blog"
    }
  ]
}
